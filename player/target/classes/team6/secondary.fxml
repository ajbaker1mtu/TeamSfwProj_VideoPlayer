<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="422.0" prefWidth="373.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="team6.SecondaryController">
    <children>
        <Label text="Select Video File" />
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
         <children>
            <ComboBox fx:id="videoDrop" nodeOrientation="LEFT_TO_RIGHT" prefWidth="150.0" promptText="Open Recent Video" visibleRowCount="5">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></ComboBox>
         </children>
      </VBox>
        <Button fx:id="secondaryButton" onAction="#switchToPrimary" text="Play Video">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="openfilebutton" mnemonicParsing="false" onAction="#openFile" text="Open Video File">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
